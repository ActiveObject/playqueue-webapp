<div
  data-audio-id="{{ aid }}"
  {{#if queued}}
    class="audio-item tracklist-item queued"
  {{else}}
    class="audio-item tracklist-item"
  {{/if}}
  draggable="true">

  <div class="title"><span>{{{ title }}}</span></div>
  <div class="artist"><span>{{{ artist }}}</span></div>
  <div class="qorder">{{ qorder }}</div>
  <div class="actions">
    <div class="btn-play" data-icon="&#xe00d"></div>
    <div class="add-to-queue" data-icon="&#xe008"></div>
  </div>
</div>